	^internalKernel ArgumentError
)
public CannotReturn = (
	^internalKernel CannotReturn
)
public Closure = (
	^internalKernel Closure
)
public Ephemeron = (
	^internalKernel Ephemeron
)
public Exception = (
	^internalKernel Exception
)
public Future = (
	^internalKernel Future
)
public Message = (
	^internalKernel Message
)
public MessageNotUnderstood = (
	^internalKernel MessageNotUnderstood
)
public Proxy = (
	^internalKernel Proxy
)
public StringBuilder = (
	^internalKernel StringBuilder
)
public WeakArray = (
	^internalKernel WeakArray
)
public WeakMap = (
	^internalKernel WeakMap
)
public garbageCollect = (
	(* for testing *)
	internalKernel garbageCollect
)
) : (
)
